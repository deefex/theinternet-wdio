{
  "name": "theinternet-wdio",
  "version": "1.0.2",
  "description": "NodeJS/wdio-v6 Web Automation",
  "main": "conf/wdio.conf.js",
  "scripts": {
    "cleanup": "rm -rf ./allure-results && rm -rf ./allure-report && rm -rf ./errorShots",
    "pretest": "npm run cleanup",
    "test": "./node_modules/.bin/wdio ./conf/wdio.conf.js",
    "report": "allure generate allure-results && allure open",
    "test:report": "npm-run-all test report"
  },
  "dependencies": {
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "fs": "^0.0.1-security",
    "html-dnd": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "webdriverio": "^6.1.22",
    "@wdio/cli": "^6.1.22",
    "@wdio/local-runner": "^6.1.22",
    "@wdio/mocha-framework": "^6.1.19",
    "@wdio/selenium-standalone-service": "^6.1.14",
    "@wdio/spec-reporter": "^6.1.14",
    "@wdio/sync": "^6.1.14"
  },
  "devDependencies": {
    "eslint": "^7.3.1"
  },
  "keywords": [
    "webdriver",
    "webdriverio",
    "wdio"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deefex/web-automation-javascript.git"
